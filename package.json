{
  "name": "back-end",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run prisma:deploy && node dist/src/server.js",
    "dev:test": "dotenv -e .env.test --exec npx ts-node src/server.ts",
    "dev": "nodemon ./src/server.ts",
    "test": "dotenv -e .env.test npx prisma migrate dev &&  cross-env NODE_OPTIONS=--experimental-vm-modules dotenv -e .env.test jest -i",
    "test:unit": "dotenv -e .env.test cross-env NODE_OPTIONS=--experimental-vm-modules jest -- -i --coverage --testPathPattern=tests/unit",
    "prisma:reset": "prisma migrate reset",
    "prisma:migrate": "prisma migrate dev"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "@supabase/supabase-js": "^1.35.6",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/joi": "^17.2.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.4",
    "@types/supertest": "^2.0.12",
    "dotenv-cli": "^6.0.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}